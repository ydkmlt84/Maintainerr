{
  "name": "@maintainerr/server",
  "version": "2.26.1",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "nest:build": "nest build",
    "start": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "ts-node --project ./tsconfig.json ../../tools/typeOrmCli.ts migration:create",
    "migration:run": "ts-node --project ./tsconfig.json ../../tools/typeOrmCli.ts migration:run -d ./src/datasource-config.ts",
    "migration:revert": "ts-node --project ./tsconfig.json ../../tools/typeOrmCli.ts migration:revert -d ./src/datasource-config.ts",
    "migration:generate": "ts-node --project ./tsconfig.json ../../tools/typeOrmCli.ts migration:generate --dataSource ./src/datasource-config.ts -p",
    "migration:show": "ts-node --project ./tsconfig.json ../../tools/typeOrmCli.ts migration:show --dataSource ./src/datasource-config.ts"
  },
  "dependencies": {
    "@maintainerr/contracts": "workspace:^",
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/typeorm": "^11.0.0",
    "@octokit/core": "^5.2.0",
    "@octokit/plugin-throttling": "^8.2.0",
    "@tygra/nestjs-graceful-shutdown": "^2.0.0",
    "@types/express": "^5.0.6",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "better-sqlite3": "^12.6.0",
    "chalk": "^4.1.2",
    "cron-validator": "^1.4.0",
    "email-templates": "13.0.1",
    "http-terminator": "^3.2.0",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.2",
    "nestjs-zod": "^5.0.1",
    "node-cache": "^5.1.2",
    "nodemailer": "7.0.12",
    "octokit": "^3.2.2",
    "openpgp": "^6.3.0",
    "read-last-lines": "^1.8.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.28",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.jest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@types/email-templates": "^10.0.4",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.23",
    "@types/mime-types": "^3",
    "@types/node": "^22",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.1",
    "jest": "^30.2.0",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@octokit|octokit|bottleneck)/)"
    ],
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../test/jest.setup.ts"
    ]
  }
}
